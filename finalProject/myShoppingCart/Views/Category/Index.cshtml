@model List<Category>

<h1>Category List</h1>
<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>
                Category Name
            </th>
            <th>
                Category Description
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach(var obj in Model)
        {
            <tr>
                <td>
                    @obj.categoryName
                </td>
                <td>
                    @obj.categoryDesc
                </td>
            </tr>
        } 
    </tbody>
</table>